{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unneeded-ternary":0,
        "no-unused-vars":"warn",
        "prefer-regex-literals":0,
        "no-useless-escape":"off"
    }
}
